<launch>
  <node pkg="usb_cam" type="usb_cam_node" name="logi_c920" output="screen" >
    <param name="video_device" value="/dev/logi_c920" />
    <param name="image_width" value="640" />
    <param name="image_height" value="480" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_frame_id" value="usb_cam" />
    <param name="io_method" value="mmap"/>
    <param name="camera_info_url" type="string" value="package://merlion_setup/camera_calibration/logi_c920.yaml" />
    <param name="camera_name" value="logitech_c920" />
  </node>
</launch>